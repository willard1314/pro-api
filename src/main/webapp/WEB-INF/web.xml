<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>pro-api</display-name>
  <servlet>
    <servlet-name>servletDemo</servlet-name>
    <servlet-class>com.willard.javaweb.servlet.ServletDemo</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>servletDemo</servlet-name>
    <url-pattern>/servletDemo.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>servletConfigDemo</servlet-name>
    <servlet-class>com.willard.javaweb.servlet.ServletConfigDemo</servlet-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>servletConfigDemo</servlet-name>
    <url-pattern>/servletConfigDemo.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>servletContextDemo</servlet-name>
    <servlet-class>com.willard.javaweb.servlet.ServletContextDemo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servletContextDemo</servlet-name>
    <url-pattern>/servletContextDemo.do</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>encoding</param-name>
    <param-value>gbk</param-value>
  </context-param>
</web-app>